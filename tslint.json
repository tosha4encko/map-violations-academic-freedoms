{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "quotemark": [true, "single"],
    "prettier": true,
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "variable-name": [true, "allow-leading-underscore"]
  },
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}